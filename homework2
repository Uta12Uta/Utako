def run_test():
    print("==== Test started! ====")
    test("1+2")
    test("1.0+2.1-3")
    test("2+3*4")
    test("2*3*4") #掛け算３連
    test("8/4*2")　#掛け算と割り算のみの場合どのように計算できているか確認する
    test("100/10+5*2")　
    test("3.8+2*2")　#小数の計算の確認
    test("10/0")　#0で割る場合
    test("1/3*3")　　
    test("10-2*3+4/2")
    test("1/3")


    print("==== Test finished! ====\n")

run_test()
